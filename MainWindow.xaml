<Window x:Class="WPF_ADO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_ADO"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <StackPanel Margin="150">
            <StackPanel Margin="10" Width="200">
                <Button Height="50" Content="Quản lý khách thuê nhà" Click="OpenKhachThueNhaForm_Click"/>
            </StackPanel>
            <StackPanel Margin="10" Width="200">
                <Button Height="50" Content="Quản lý hợp đồng" Click="OpenHopDongForm_Click"/>
            </StackPanel>
        </StackPanel>

    </Grid>
</Window>
